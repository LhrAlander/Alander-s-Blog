{
  "defaultSeverity": "warn",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "never",
      "ignore-interfaces"
    ],
    "indent": [
      true,
      "tabs",
      2
    ],
    "typedef-whitespace": [
      false
    ],
    "whitespace": [
      false
    ],
    "member-access": false,
    "one-line": false,
    "ordered-imports": false,
    "member-ordering": false,
    "prefer-const": false,
    "eofline": false,
    "no-trailing-whitespace": false,
    "one-variable-per-declaration": false,
    "no-consecutive-blank-lines": false,
    "no-console": false,
    "comment-format": false,
    "no-empty": false,
    "variable-name": false,
    "max-line-length": false,
    "object-literal-sort-keys": false,
    "no-unused-expression": false,
    "no-var-requires": false,
    "arrow-parens": false,
    "no-empty-interface": false,
    "prefer-for-of": false,
    "forin": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "ignore",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  },
  "rulesDirectory": []
}